<%= turbo_stream.append "#{dom_id(@story)}_pins" do %>
  <%= render partial: 'pins/pin', locals: { pin: @pin } %>
<% end %>

<%= turbo_stream.replace "#{dom_id(@comment)}_pin" do %>
  <%= render partial: 'pins/button/unpin', locals: { comment: @comment } %>
<% end %>

<%= turbo_stream.prepend('flash', partial: 'shared/flash') %>
